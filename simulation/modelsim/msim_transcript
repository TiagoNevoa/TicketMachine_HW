force -freeze sim:/keycontrol/CLK 1 0, 0 {50 ps} -r 100
add wave -position insertpoint  \
sim:/keycontrol/CLK \
sim:/keycontrol/Kack \
sim:/keycontrol/Kpress \
sim:/keycontrol/Reset \
sim:/keycontrol/Kval \
sim:/keycontrol/Kscan \
sim:/keycontrol/Ksave \
sim:/keycontrol/CurrentState \
sim:/keycontrol/NextState
force -freeze sim:/keycontrol/Kack 0 0
force -freeze sim:/keycontrol/Kpress 0 0
force -freeze sim:/keycontrol/Reset 1 0
run
force -freeze sim:/keycontrol/Reset 0 0
run
force -freeze sim:/keycontrol/Kpress 1 0
run
force -freeze sim:/keycontrol/Kack 1 0
run
force -freeze sim:/keycontrol/Kack 0 0
run
force -freeze sim:/keycontrol/Kpress 0 0
run
